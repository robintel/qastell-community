{
  "name": "qastell-webdriverio-allure-example",
  "version": "1.1.0",
  "description": "Example: QAstell with WebDriverIO and Allure reporting",
  "scripts": {
    "test": "wdio run wdio.conf.ts",
    "test:headed": "wdio run wdio.conf.ts --headless=false",
    "allure:generate": "npx allure generate allure-results -o allure-report --clean",
    "allure:open": "npx allure open allure-report",
    "report": "npm run allure:generate && npm run allure:open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf allure-results allure-report qastell-report .allure"
  },
  "dependencies": {
    "@wdio/allure-reporter": "^9.0.0",
    "@wdio/cli": "^9.0.0",
    "@wdio/local-runner": "^9.0.0",
    "@wdio/mocha-framework": "^9.0.0",
    "@wdio/spec-reporter": "^9.0.0",
    "allure-commandline": "^2.32.0",
    "expect-webdriverio": "^5.0.0",
    "qastell": "^0.9.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "webdriverio": "^9.0.0"
  }
}
