{
  "name": "qastell-playwright-cucumber-multi-example",
  "version": "1.1.0",
  "description": "Example: QAstell with Playwright, Cucumber BDD, and Multiple Cucumber HTML Reporter",
  "scripts": {
    "test": "cucumber-js",
    "test:dry": "cucumber-js --dry-run",
    "report": "node scripts/generate-report.js",
    "test:report": "npm test && npm run report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf reports qastell-report"
  },
  "dependencies": {
    "@cucumber/cucumber": "^11.0.0",
    "@playwright/test": "^1.57.0",
    "multiple-cucumber-html-reporter": "^3.8.0",
    "qastell": "^0.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
