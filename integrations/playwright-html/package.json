{
  "name": "qastell-playwright-html-example",
  "version": "1.0.0",
  "description": "Example: QAstell with Playwright and HTML reporting",
  "scripts": {
    "setup": "cd ../../.. && npm run build && npm link && cd examples/integrations/playwright-html && npm link qastell",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "report": "playwright show-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf playwright-report test-results qastell-report"
  },
  "dependencies": {
    "@playwright/test": "^1.50.0",
    "qastell": "file:../../.."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.3.0"
  }
}
